! COLOR SCHEME
! https://github.com/joshdick/onedark.vim/blob/master/term/One%20Dark.Xresources
! special
URxvt*foreground:   #ABB2BF
URxvt*background:   #282C34
URxvt*cursorColor:  #ABB2BF
! black
URxvt*color0:       #282C34
URxvt*color8:       #3E4452
! red
URxvt*color1:       #E06C75
URxvt*color9:       #BE5046
! green
URxvt*color2:       #98C379
URxvt*color10:      #98C379
! yellow
URxvt*color3:       #E5C07B
URxvt*color11:      #D19A66
! blue
URxvt*color4:       #61AFEF
URxvt*color12:      #61AFEF
! magenta
URxvt*color5:       #C678DD
URxvt*color13:      #C678DD
! cyan
URxvt*color6:       #56B6C2
URxvt*color14:      #56B6C2
! white
URxvt*color7:       #ABB2BF
URxvt*color15:		#5C6370

! FONTS
URxvt*allow_bold: true
URxvt*font: xft:Fira Code:pixelsize=17:style=Regular
URxvt*boldFont: xft:Fira Code:pixelsize=17:style=Bold
URxvt*italicFont: xft:Fira Code:pixelsize=17:style=Italic
URxvt*boldItalicFont: xft:Fira Code:pixelsize=17:style=Bold Italic

! APPEARANCE
URxvt*letterSpace: 0
URxvt*lineSpace: 0
URxvt*geometry: 92x24
URxvt*internalBorder: 6
URxvt*cursorBlink: true
URxvt*cursorUnderline: false
URxvt*saveline: 2048
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*urgentOnBell: true
URxvt*depth: 32
URxvt*iso14755: false

! EXTENSIONS
URxvt*perl-ext-common: default,matcher,clipboard
URxvt*url-launcher: /usr/bin/xdg-open
URxvt*matcher.button: 1
URxvt*clipboard.autocopy: true
URxvt*keysym.Shift-Control-V: eval:paste_clipboard
URxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
